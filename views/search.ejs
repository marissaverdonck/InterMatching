      <section id="interesses">
        <h3> Interests</h3>
        <p> We create matches with interests. Which interests does your dream match have? Choose the one you like and get matched! </p>

        <article>
          <img src="images/climbing.png" alt="interest climbing" class="interest">
          <h4>Climbing</h4>
          <div>
            <input name="like" id="like" type="radio" value="like" />
            <input name="like" id="dislike" type="radio" value="dislike" />
          </div>
        </article>

        <% for (var l = 0; l < data.length; l++) {%>
          <% var user = data[l]; %>

          <% if (user.interests != null) {%>
            <% for (var i = 0; i < user.interests.length; i++) {%>
              
              <article>
                <!-- <%= user.pictures[i].path %> -->
                <img src="https://loremflickr.com/320/240" alt="interest climbing" class="interest">
                
                <h4><%= user.interests[i] %></h4>
                <div>
                  <input name="like" id="like" type="radio" value="like" />
                  <input name="like" id="dislike" type="radio" value="dislike" />
                </div>
              </article>
            <% } %>
